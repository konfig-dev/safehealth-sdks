{
  "openapi": "3.0.1",
  "info": {
    "title": "Safe Push API",
    "description": "Send push notifications to a user's device\n\n[Health Check UI](http://localhost:8012/healthchecks-ui)",
    "version": "0.3.1-9ffa049"
  },
  "servers": [
    {
      "url": "https://api.dev-1.sf.safecdx.io/v1beta/notification-push"
    }
  ],
  "tags": [
    {
      "name": "Device"
    },
    {
      "name": "UserDevice"
    }
  ],
  "paths": {
    "/users/{userId}/devices": {
      "post": {
        "tags": [
          "Device"
        ],
        "summary": "Stores a new device associated with the specified user. If a device with any matching token(s) already exists for that user, it will be removed.",
        "operationId": "Device_store",
        "parameters": [
          {
            "description": "Identifies the user to whom this device will be associated.",
            "name": "userId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "description": "Carries all device tokens and information required to deliver notifications to the device.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DeviceCreateRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/DeviceCreateRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/DeviceCreateRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceCreateResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceCreateResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceCreateResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceStoreResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceStoreResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceStoreResponse"
                }
              }
            }
          },
          "500": {
            "description": "Server Error"
          }
        }
      },
      "get": {
        "tags": [
          "Device"
        ],
        "summary": "Retrieves a pageable list of devices associated with the specified user.",
        "operationId": "Device_list",
        "parameters": [
          {
            "description": "Identifies the user for whom to retrieve devices.",
            "name": "userId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "Page",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "Limit",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceListResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceListResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceListResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceList400Response"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceList400Response"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeviceList400Response"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/users/{userId}/devices/{deviceId}": {
      "delete": {
        "tags": [
          "Device"
        ],
        "summary": "Deletes the identified device from the specified user's devices.",
        "operationId": "Device_delete",
        "parameters": [
          {
            "description": "Identifies the user to whom the device is associated.",
            "name": "userId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "description": "Identifies the device to be deleted.",
            "name": "deviceId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "404": {
            "description": "Not Found",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/device": {
      "post": {
        "tags": [
          "UserDevice"
        ],
        "summary": "Creates a UserDevice entity.",
        "operationId": "UserDevice_create",
        "requestBody": {
          "description": "Represents a device used by the legacy push notification system.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserDevice"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UserDevice"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UserDevice"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/UserDevice"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserDevice"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserDevice"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "500": {
            "description": "Server Error"
          }
        }
      },
      "delete": {
        "tags": [
          "UserDevice"
        ],
        "summary": "Deletes UserDevice entity by device token.",
        "operationId": "UserDevice_delete",
        "parameters": [
          {
            "description": "Identifying token passed when creating UserDevice.",
            "name": "deviceToken",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/UserDevice"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserDevice"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserDevice"
                }
              }
            }
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    }
  },
